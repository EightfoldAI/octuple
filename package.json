  "scripts": {
    "start": "react-scripts start",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 2022",
    "build:storybook": "rm -rf dist && mkdir dist && build-storybook -o dist",
    "build": "rm -rf lib && eslint . --ext .js,.jsx,.ts,.tsx && webpack --config webpack.config.js --mode production",
    "details": "webpack --config webpack.config.js --stats-error-details",
    "analyze": "webpack --config webpack.config.js --mode production --json > stats.json",
    "prepublish": "yarn build",
    "test": "jest"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
