{
    "name": "@eightfold.ai/octuple",
    "version": "2.0.0",
    "license": "MIT",
    "main": "lib/octuple.js",
    "types": "lib/octuple.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/EightfoldAI/octuple"
    },
    "scripts": {
        "storybook": "start-storybook -p 2022",
        "build-storybook": "rm -rf dist && mkdir dist && build-storybook -o dist",
        "build": "rm -rf lib && npm run lint && webpack --config webpack.prod.js --mode production",
        "details": "webpack --config webpack.dev.js --mode development --stats-error-details",
        "analyze": "webpack --config webpack.dev.js --mode production --json > stats.json",
        "prepublish": "yarn build",
        "test": "jest",
        "test:update": "jest -u",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "format": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|css|scss|md)\"",
        "chromatic": "chromatic --exit-zero-on-changes",
        "prepare": "husky install && install-peers",
        "lint-staged": "lint-staged",
        "release": "standard-version",
        "release:minor": "standard-version --release-as minor",
        "release:patch": "standard-version --release-as patch",
        "release:major": "standard-version --release-as major"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "@floating-ui/react-dom": "0.6.0",
        "@floating-ui/react-dom-interactions": "0.6.3",
        "@mdi/react": "1.5.0",
        "bodymovin": "4.13.0",
        "lottie-web": "5.8.1",
        "react-flip-toolkit": "7.0.13"
    },
    "peerDependencies": {
        "@types/react": "17.0.39",
        "@types/react-dom": "17.0.11",
        "react": "17.0.2",
        "react-app-polyfill": "3.0.0",
        "react-dom": "17.0.2"
    },
    "devDependencies": {
        "@babel/core": "7.17.5",
        "@babel/runtime": "7.17.2",
        "@commitlint/cli": "16.2.3",
        "@commitlint/config-conventional": "16.2.1",
        "@ctrl/tinycolor": "3.4.0",
        "@pmmmwh/react-refresh-webpack-plugin": "0.5.3",
        "@storybook/addon-actions": "6.5.5",
        "@storybook/addon-docs": "6.5.5",
        "@storybook/addon-essentials": "6.5.5",
        "@storybook/addon-info": "6.0.0-alpha.2",
        "@storybook/addon-interactions": "6.5.5",
        "@storybook/addon-links": "6.5.5",
        "@storybook/addon-postcss": "2.0.0",
        "@storybook/addon-storyshots": "6.5.5",
        "@storybook/addon-viewport": "6.5.5",
        "@storybook/addons": "6.5.5",
        "@storybook/builder-webpack5": "6.5.5",
        "@storybook/manager-webpack5": "6.5.5",
        "@storybook/node-logger": "6.5.5",
        "@storybook/preset-scss": "1.0.3",
        "@storybook/react": "6.5.5",
        "@storybook/source-loader": "6.5.5",
        "@storybook/theming": "6.5.5",
        "@svgr/webpack": "5.5.0",
        "@teamsupercell/typings-for-css-modules-loader": "2.1.0",
        "@testing-library/jest-dom": "5.16.2",
        "@testing-library/react": "12.1.3",
        "@testing-library/react-hooks": "7.0.2",
        "@testing-library/user-event": "13.5.0",
        "@types/dotenv-webpack": "7.0.3",
        "@types/enzyme": "3.10.4",
        "@types/jest": "24.0.23",
        "@types/node": "16.11.26",
        "@types/react-color": "3.0.6",
        "@types/webpack": "5.28.0",
        "@typescript-eslint/eslint-plugin": "5.14.0",
        "@typescript-eslint/parser": "5.14.0",
        "@wojtekmaj/enzyme-adapter-react-17": "0.3.2",
        "babel-jest": "27.4.2",
        "babel-loader": "8.2.3",
        "babel-plugin-named-asset-import": "0.3.8",
        "babel-preset-react-app": "10.0.1",
        "bfj": "7.0.2",
        "browserslist": "4.18.1",
        "camelcase": "6.2.1",
        "case-sensitive-paths-webpack-plugin": "2.4.0",
        "chromatic": "6.5.4",
        "clean-webpack-plugin": "3.0.0",
        "copy-webpack-plugin": "6.0.1",
        "css-loader": "6.5.1",
        "css-minimizer-webpack-plugin": "3.4.1",
        "cssnano": "4.1.10",
        "dotenv": "10.0.0",
        "dotenv-expand": "5.1.0",
        "dotenv-webpack": "7.0.3",
        "enzyme": "3.10.0",
        "eslint": "8.10.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-config-react-app": "7.0.0",
        "eslint-plugin-prettier": "4.0.0",
        "eslint-plugin-storybook": "0.5.12",
        "eslint-webpack-plugin": "3.1.1",
        "file-loader": "6.2.0",
        "fs-extra": "10.0.0",
        "html-replace-webpack-plugin": "2.5.6",
        "html-webpack-plugin": "5.5.0",
        "husky": "7.0.4",
        "icomoon-react": "^3.0.0",
        "identity-obj-proxy": "3.0.0",
        "install-peers": "1.0.3",
        "install-peers-cli": "2.2.0",
        "jest": "27.4.3",
        "jest-localstorage-mock": "2.4.19",
        "jest-matchmedia-mock": "1.1.0",
        "jest-resolve": "27.4.2",
        "jest-watch-typeahead": "1.0.0",
        "lint-staged": "12.3.6",
        "mini-css-extract-plugin": "2.6.0",
        "postcss": "8.4.4",
        "postcss-flexbugs-fixes": "5.0.2",
        "postcss-loader": "6.2.1",
        "postcss-normalize": "10.0.1",
        "postcss-preset-env": "7.0.1",
        "prettier": "2.5.1",
        "prompts": "2.4.2",
        "react-color": "2.19.3",
        "react-dev-utils": "12.0.0",
        "react-docgen-typescript": "2.2.2",
        "react-refresh": "0.11.0",
        "react-test-renderer": "17.0.2",
        "resolve": "1.20.0",
        "resolve-url-loader": "4.0.0",
        "sass": "1.47.0",
        "sass-loader": "12.3.0",
        "semver": "7.3.5",
        "source-map-loader": "3.0.0",
        "standard-version": "9.3.2",
        "storybook-addon-state": "1.0.3",
        "storybook-css-modules": "^1.0.5",
        "style-loader": "3.3.1",
        "style-resources-loader": "1.5.0",
        "terser-webpack-plugin": "5.2.5",
        "ts-jest": "26.4.0",
        "ts-loader": "8.0.4",
        "tsconfig-paths-jest": "0.0.1",
        "tsconfig-paths-webpack-plugin": "3.5.1",
        "tslib": "2.1.0",
        "typescript": "4.6.2",
        "typescript-plugin-css-modules": "3.4.0",
        "url-loader": "4.1.1",
        "webpack": "5.64.4",
        "webpack-bundle-analyzer": "4.5.0",
        "webpack-cli": "4.9.2",
        "webpack-dev-middleware": "4.1.0",
        "webpack-dev-server": "4.6.0",
        "webpack-manifest-plugin": "4.0.2",
        "webpack-virtual-modules": "0.4.2",
        "window-resizeto": "0.0.2",
        "workbox-webpack-plugin": "6.4.1"
    },
    "jest": {
        "roots": [
            "<rootDir>/src"
        ],
        "setupFiles": [
            "react-app-polyfill/jsdom",
            "jest-localstorage-mock"
        ],
        "setupFilesAfterEnv": [
            "window-resizeto/polyfill"
        ],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
            "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
        ],
        "testEnvironment": "jsdom",
        "transform": {
            "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
            "^.+\\.module\\.(css|sass|scss)$"
        ],
        "modulePaths": [],
        "moduleNameMapper": {
            "^react-native$": "react-native-web",
            "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
        },
        "moduleFileExtensions": [
            "web.js",
            "js",
            "web.ts",
            "ts",
            "web.tsx",
            "tsx",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ],
        "watchPlugins": [
            "jest-watch-typeahead/filename",
            "jest-watch-typeahead/testname"
        ],
        "resetMocks": false,
        "collectCoverage": true,
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!src/**/*.d.ts",
            "!src/**/*.stories.{js,jsx,ts,tsx}",
            "!src/**/*.types.{js,ts}"
        ]
    },
    "babel": {
        "presets": [
            "react-app"
        ]
    },
    "lint-staged": {
        "*.+(js|jsx|ts|tsx)": [
            "eslint --fix"
        ],
        "*.+(js|jsx|ts|tsx|json|css|scss|md)": [
            "prettier --write"
        ]
    },
    "engines": {
        "node": ">=16.14.0 < 16.15",
        "npm": ">=8.5.0 < 8.6"
    }
}
